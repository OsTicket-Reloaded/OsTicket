{
    "root": "docs",
    "title": "OsTicket Reload - User Manual",
    "structure": {
        "readme": "readme.md",
        "summary": "summary.md"
    },
    "plugins": [
        "disqus",
        "layout",
        "anchor-navigation",
        "anchors",
		"toc",
        "page-toc"
    ],
    "links": {
        "sidebar": {
            "Site": "http://carlosdelfino.eti.br"
        }
    },
    "pluginsConfig": {
        "disqus": {
            "shortName": "osticket",
            "useIdentifier": true
        },
        "page-toc": {
            "selector": ".markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first"
        }
    }
}
