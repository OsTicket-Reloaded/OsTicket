{
    "root": "docs",
    "title": "OsTicket Reload - User Manual",
    "language": "pt",    
    "repository": {
        "type": "git",
        "url": "git@github.com:OsTicket-Reloaded/OsTicket.git"
    },
    "plugins": [
        "disqus",
        "layout",
        "anchor-navigation",
        "anchors",
		"toc",
        "page-toc"
    ],
    "links": {
        "sidebar": {
            "Site": "http://osticket-reloaded.github.io"
        }
    },
    "pluginsConfig": {
        "disqus": {
            "shortName": "osticket-reload",
            "useIdentifier": true
        },
        "page-toc": {
            "selector": ".markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first"
        }
    }
}
